{
  "name": "homkaapp",
  "version": "0.0.1",
  "description": "homkaapp with homka store",
  "main": "./dist/main.prod.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dist1": " electron-builder --dir",
    "dist": "electron-builder --publish always",
    "dev": "npm run start-renderer",
    "prod": "run-electron .",
    "build": "electron-builder",
    "build-main": "webpack --config webpack/main.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js",
    "start-main": "cross-env NODE_ENV=development run-electron -r @babel/register src/main/index.js",
    "start-renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack/renderer.dev.config.js"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "Syoma Jenya",
  "repository": "https://github.com/pporc/homkaapp",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.1",
    "electron-builder": "^22.8.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "run-electron": "^0.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "electron-log": "^4.2.4",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.5",
    "react": "^16.8.5",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.8.5"
  },
  "build": {
    "appId": "com.electron.homkaapp",
    "publish": {
      "provider": "github"
    },
    "copyright": "Copyright 2020 Syoma Jenya",
    "directories": {
      "output": "build"
    },
    "win": {
      "files": [
        "./dist/**/*"
      ],
      "target": "nsis",
      "icon": "resources/icon.ico"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}
